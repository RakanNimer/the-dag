{
  "include": ["./src/"],
  "compilerOptions": {
    /* Basic Options */
    "allowSyntheticDefaultImports": true,
    "rootDir": "./src",
    "outDir": "./build/",
    // "jsx": "react",
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2016",
    // "noEmit": true,
    // "noUnusedLocals": true,
    // "noImplicitAny": false,
    // "noUnusedParameters": true,
    "skipLibCheck": true,
    "pretty": true
  }
}
