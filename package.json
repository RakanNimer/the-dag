{
  "name": "the-dag",
  "version": "0.2.0",
  "description": "Yet Another Directed Acyclic Graph Implementation ",
  "main": "umd/bundle.js",
  "browser": "umd/bundle.js",
  "repository": "https://www.github.com/rakannimer/the-dag",
  "author": "Rakan Nimer",
  "license": "MIT",
  "scripts": {
    "precommit": "NODE_ENV=testing npm run prettier && npm run lint  && npm run test && npm run docs",
    "docs": "jsdoc src/TheDAG.js -c .jsdoc.json -R README.md -d docs/",
    "test": "NODE_ENV=testing jest",
    "test-ci": "NODE_ENV=testing npm run lint && jest --runInBand --coverage",
    "prettier": "prettier --write --single-quote src/core/**/*.js src/*.js",
    "lint": "eslint src/core/**/*.js src/*.js",
    "build": "NODE_ENV=production rollup -c"
  },
  "jest": {
    "setupFiles": [
      "./jestSetup.js"
    ],
    "collectCoverageFrom": [
      "src/core/**/*.js",
      "src/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "eslint": "^4.5.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jsdoc": "^3.5.4",
    "prettier": "^1.5.3",
    "regenerator-runtime": "^0.11.0",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-cleanup": "^1.0.1"
  },
  "dependencies": {
    "fifo": "^2.3.0",
    "toposort": "^1.0.3",
    "uuid": "^3.1.0"
  }
}
